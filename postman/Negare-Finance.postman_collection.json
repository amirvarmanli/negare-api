{
  "info": {
    "name": "Negare Finance",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    { "key": "API_BASE", "value": "http://localhost:4000/api" },
    { "key": "ACCESS_TOKEN", "value": "" },
    { "key": "productId", "value": "" },
    { "key": "ORDER_ID", "value": "" },
    { "key": "PAYMENT_ID", "value": "" },
    { "key": "cartItemId", "value": "" },
    { "key": "trackId", "value": "" },
    { "key": "poolId", "value": "" },
    { "key": "payoutId", "value": "" },
    { "key": "supplierId", "value": "" },
    { "key": "purchaseProductId", "value": "" },
    { "key": "fileId", "value": "" },
    { "key": "downloadToken", "value": "" },
    { "key": "DONATION_ID", "value": "" }
