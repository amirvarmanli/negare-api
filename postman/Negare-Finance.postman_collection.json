  "item": [
    {
      "name": "Orders",
      "item": [
        {
          "name": "Create Order",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Authorization", "value": "Bearer {{ACCESS_TOKEN}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": "{{API_BASE}}/orders",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"items\": [{\n    \"productId\": \"1024\",\n    \"quantity\": 1\n  }],\n  \"discount\": {\n    \"type\": \"FIXED\",\n    \"value\": 50000\n  }\n}"
            }
          }
        },
        {
          "name": "Get Order",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{ACCESS_TOKEN}}" }
            ],
            "url": "{{API_BASE}}/orders/{{ORDER_ID}}"
          }
        },
        {
          "name": "Get Purchase Result",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{ACCESS_TOKEN}}" }
            ],
            "url": "{{API_BASE}}/orders/{{ORDER_ID}}/purchase-result"
          }
        },
        {
          "name": "Pay Order via Gateway Init",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Authorization", "value": "Bearer {{ACCESS_TOKEN}}" }
            ],
            "url": "{{API_BASE}}/orders/{{ORDER_ID}}/pay/gateway/init"
          }
        },
        {
          "name": "Pay Order via Wallet",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('order paid', () => {",
                  "  const body = pm.response.json();",
                  "  pm.expect(body.status).to.eql('PAID');",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              { "key": "Authorization", "value": "Bearer {{ACCESS_TOKEN}}" }
            ],
            "url": "{{API_BASE}}/orders/{{ORDER_ID}}/pay-with-wallet"
          }
        }
      ]
    },
    {
      "name": "Me Purchases",
      "item": [
        {
          "name": "Get Purchases",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "const body = pm.response.json();",
                  "const first = body?.data?.items?.[0];",
                  "if (first) {",
                  "  pm.collectionVariables.set('purchaseProductId', first.productId);",
                  "  const dl = first.downloads?.[0];",
                  "  if (dl) {",
                  "    pm.collectionVariables.set('fileId', dl.fileId);",
                  "    const url = dl.url || '';",
                  "    const token = url.includes('token=') ? url.split('token=')[1] : '';",
                  "    if (token) pm.collectionVariables.set('downloadToken', token);",
                  "  }",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{ACCESS_TOKEN}}" }
            ],
            "url": "{{API_BASE}}/me/purchases?page=1&pageSize=20"
          }
        },
        {
          "name": "Get Purchase By Product",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{ACCESS_TOKEN}}" }
            ],
            "url": "{{API_BASE}}/me/purchases/{{purchaseProductId}}"
          }
        }
      ]
    },
    {
      "name": "Negative Cases",
