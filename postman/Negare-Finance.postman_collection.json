            "url": "{{API_BASE}}/cart/items",
            "body": {
              "mode": "raw",
              "raw": "{\\n  \\\"productId\\\": \\\"{{purchaseProductId}}\\\",\\n  \\\"quantity\\\": 1\\n}"
